#include<iostream>
using namespace std;
int main()
{
	int ar[3][3],i,j,count;
	cout<<"element";
	for(i=0;i<=3;i++)
			for(j=0;j<=3;j++)
				cin>>ar[i][j];
	
	for(i=0;i<=3;i++)
		for(j=0;j<=3;j++)
			if(ar[i][j]%7==0)
			{
				count=count+1;
			    cout<<"Total elements are"<<count;
			}       
		return 0;
}
